{
  "name": "printd",
  "version": "1.4.0",
  "description": "Print HTML elements in modern browsers.",
  "repository": "joseluisq/printd",
  "license": "MIT",
  "author": {
    "name": "JosÃ© Quintana",
    "url": "http://git.io/joseluisq"
  },
  "keywords": [
    "html",
    "dom",
    "print",
    "elements",
    "element printing"
  ],
  "engines": {
    "node": ">= 4"
  },
  "main": "dist/printd.js",
  "source": "source/index.ts",
  "module": "dist/printd.mjs",
  "unpkg": "dist/printd.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "bundlesize": [
    {
      "path": "./index.js",
      "maxSize": "1.6KB"
    },
    {
      "path": "./*.min.js",
      "maxSize": "1.6KB"
    }
  ],
  "scripts": {
    "version": "npm run build",
    "build": "microbundle --no-sourcemap",
    "prebuild": "yarn clean",
    "clean": "rm -rf dist .fusebox .cache",
    "test": "npm run lint && karma start config/karma.conf.js --single-run --reporters spec",
    "test:bundle": "yarn build && bundlesize",
    "test:watch": "env NODE_ENV=debug karma start config/karma.conf.js",
    "lint": "tslint --format stylish --project tsconfig.json",
    "sample": "yarn clean && env NODE_ENV=development node config/fusebox.bundle.conf.js",
    "sample:build": "yarn clean && env NODE_ENV=production node config/fusebox.bundle.conf.js dist"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.12",
    "@types/node": "8.10.48",
    "bundlesize": "0.17.2",
    "fuse-box": "^3.7.1",
    "git-testing-hook": "0.3.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "4.0.0",
    "microbundle": "^0.11.0",
    "node-sass": "^4.12.0",
    "terser": "^4.0.0",
    "tslint": "^5.16.0",
    "tslint-config-standard-plus": "^2.1.4",
    "typescript": "^3.4.5"
  }
}
